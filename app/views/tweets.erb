<h1> Welcome, <%= @user.username %> </h1>
<br>
<br>

  <ul>
    <% Tweet.all.each do |t| %>
    <% if t.user_id == session[:user_id] %>
    <li>
      <a href="/tweets/<%= t.id %>">
      <%= t.content %>
      </a>
    </li>
      <% end %>
    <% end %>
  </ul>

<br>
<br>
<a href="/tweets/new">New Tweet!</a>
